<h2>Enter a new dinosaur to submit into inventory:</h2>
<%= form_tag '/products', method: :post do %>
  <div class="form-group">
    <%= label_tag :title %>
    <%= text_field_tag :title, nil, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= label_tag :description %>
    <%= text_field_tag :description, nil, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= label_tag :price %>
    <%= text_field_tag :price, nil, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= label_tag :quantity %>
    <%= text_field_tag :quantity, nil, class: "form-control" %>
  </div>
  <div>
    <%= button_tag 'Submit', class: "btn btn-default" %>
  </div>
<% end %>